<h1>Your Favorites</h1>
<div class="container yourpost">
  <div class="row">
    <% current_user.favorite_pictures.order(created_at: :desc).each do |picture| %>
      <div class="col-xs-4">
        <%= link_to (image_tag picture.image.url), picture, class: "mypics"%>
      </div>
    <% end %> 
  </div>
</div>